ssh aubwstsdnt002 
df -k | grep UVH

[ ! -d /UVH ] && mkdir /UVH
mount 139.73.190.120:/migration1 /UVH

#create flash archive: (on physical) 4.9G 7m16s
flarcreate -S -n aubwstsdnt002-full \
-x /UVH \
/UVH/sts/aubwstsdnt002-2014-05-16.flasharchive

umount /UVH
rmdir /UVH
df -k | grep UVH


