set type=lofs
end
add net
set address=134.251.163.143/24
set physical=igb10
set defrouter=134.251.163.1
end
add net
set address=139.73.190.12/24
set physical=igb10
end
add net
set address=205.239.124.33/24
set physical=igb10
end
set hostid=830502bd
verify


create zone:
zonecfg -z aubwtsdnt002 -f /UVH/etc/zone-create.aubwtsdnt002

